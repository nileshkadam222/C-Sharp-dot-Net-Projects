<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ValidateSer</servlet-name>
        <servlet-class>ser.ValidateSer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Ser</servlet-name>
        <servlet-class>ser.Admin_Ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>ser.Test</servlet-class>
        <init-param>
            <param-name>do</param-name>
            <param-value/>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Client_Ser</servlet-name>
        <servlet-class>ser.Client_Ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Visit_Ser</servlet-name>
        <servlet-class>ser.Visit_Ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cust_Ser</servlet-name>
        <servlet-class>ser.Cust_Ser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ValidateSer</servlet-name>
        <url-pattern>/ValidateSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Ser</servlet-name>
        <url-pattern>/AdminSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Client_Ser</servlet-name>
        <url-pattern>/ClientSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Visit_Ser</servlet-name>
        <url-pattern>/VisitSer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cust_Ser</servlet-name>
        <url-pattern>/CustSer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
